{
    "openapi": "3.0.0",
    "info": {
        "title": "Barrio Fino Peluqueria API",
        "description": "API documentation for Barrio Fino Peluqueria application",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "Local Development Server"
        }
    ],
    "paths": {
        "/api/v1/register": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Register a new user",
                "operationId": "ad77fe138bcb384ad65d167d5c8d1cc0",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password",
                                    "password_confirmation"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "John Doe"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "doe@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User registered successfully"
                    },
                    "409": {
                        "description": "Email already exists"
                    },
                    "422": {
                        "description": "Validation failed"
                    }
                }
            }
        },
        "/api/v1/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Login a user and obtain an access token",
                "operationId": "bae68d1c55329ecb6e41f48285840c8f",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "regusa2008@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User logged in successfully"
                    },
                    "401": {
                        "description": "Invalid credentials"
                    },
                    "422": {
                        "description": "Validation failed"
                    }
                }
            }
        },
        "/api/v1/profile": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "Get the authenticated users details",
                "operationId": "e0b0bdd253ede0aa61928f622ca1c86f",
                "responses": {
                    "200": {
                        "description": "User details retrieved successfully"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/v1/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout the authenticated user",
                "operationId": "c5cdd87fdbf9d0f95dede875bd023dec",
                "responses": {
                    "200": {
                        "description": "User logged out successfully"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "api/v1/services": {
            "get": {
                "tags": [
                    "Services"
                ],
                "summary": "Get a list of all services",
                "operationId": "c176b369e8929e216def91f5ebe3f7fb",
                "responses": {
                    "200": {
                        "description": "Successfull operation"
                    }
                }
            }
        },
        "/api/v1/services/store": {
            "post": {
                "tags": [
                    "Services"
                ],
                "summary": "Create a new service",
                "operationId": "70e82f7755a6aa0b8cac670fa2ea067a",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nombre",
                                    "precio",
                                    "descripcion",
                                    "duracion"
                                ],
                                "properties": {
                                    "nombre": {
                                        "type": "string",
                                        "example": "Corte de cabello"
                                    },
                                    "precio": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 25.5
                                    },
                                    "descripcion": {
                                        "type": "string",
                                        "example": "Corte de cabello para hombres y mujeres"
                                    },
                                    "duracion": {
                                        "type": "integer",
                                        "example": 30
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Service created successfully"
                    },
                    "422": {
                        "description": "Validation failed"
                    }
                }
            }
        },
        "api/v1/services/put/{id}": {
            "put": {
                "tags": [
                    "Services"
                ],
                "summary": "Update an existing service",
                "operationId": "64333f74948c40568c79ccf1be3aa087",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the service to update",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "nombre": {
                                        "type": "string",
                                        "example": "Corte de cabello actualizado"
                                    },
                                    "precio": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 30
                                    },
                                    "descripcion": {
                                        "type": "string",
                                        "example": "Corte de cabello actualizado para hombres y mujeres"
                                    },
                                    "duracion": {
                                        "type": "integer",
                                        "example": 45
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Service updated successfully"
                    },
                    "404": {
                        "description": "Service not found"
                    },
                    "422": {
                        "description": "Validation failed"
                    }
                }
            }
        },
        "api/v1/services/delete/{id}": {
            "delete": {
                "tags": [
                    "Services"
                ],
                "summary": "Delete a service",
                "operationId": "cfaaf00c111dca0609cce5c933309264",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the service to delete",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Service deleted successfully"
                    },
                    "404": {
                        "description": "Service not found"
                    }
                }
            }
        },
        "api/v1/services/search/{nombre}": {
            "get": {
                "tags": [
                    "Services"
                ],
                "summary": "Search services by nombre",
                "operationId": "c8cd124b9984c31319bf8c4ea598d2a0",
                "parameters": [
                    {
                        "name": "nombre",
                        "in": "path",
                        "description": "Nombre of the service to search for",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "Corte"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Services found successfully"
                    },
                    "404": {
                        "description": "No services found"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Authentication",
            "description": "Authentication"
        },
        {
            "name": "Auth",
            "description": "Auth"
        },
        {
            "name": "Services",
            "description": "Services"
        }
    ]
}