        "/api/v1/HorariosBarbero": {
            "get": {
                "tags": [
                    "Barber Schedule"
                ],
                "summary": "List barber schedules with filters",
                "description": "Returns barber schedules filtered by barber, day of week, status and time range.",
                "operationId": "c55ac5abc3cc1bf1f16aa9b03babdc98",
                "parameters": [
                    {
                        "name": "barbero_id",
                        "in": "query",
                        "description": "Filter by barber ID",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    },
                    {
                        "name": "barbero",
                        "in": "query",
                        "description": "Filter by barber name (partial match)",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "Juan"
                        }
                    },
                    {
                        "name": "dia_semana",
                        "in": "query",
                        "description": "Day of week (1=Lunes, 7=Domingo)",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "maximum": 7,
                            "minimum": 1,
                            "example": 1
                        }
                    },
                    {
                        "name": "estado",
                        "in": "query",
                        "description": "Schedule status",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "disponible",
                                "no_disponible"
                            ],
                            "example": "disponible"
                        }
                    },
                    {
                        "name": "desde",
                        "in": "query",
                        "description": "Filter schedules that end after this time",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "time",
                            "example": "09:00"
                        }
                    },
                    {
                        "name": "hasta",
                        "in": "query",
                        "description": "Filter schedules that start before this time",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "time",
                            "example": "18:00"
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Pagination page",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response"
                    },
                    "422": {
                        "description": "Invalid parameters"
                    }
                }
            }
        },
